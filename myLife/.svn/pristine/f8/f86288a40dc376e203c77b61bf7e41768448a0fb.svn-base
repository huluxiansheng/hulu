package com.myLife.system.security.entity;

import java.sql.Timestamp;
/**
 * 用户实体
 * @author HuYang 
 * @date 2016年11月7日 下午2:16:25
 */
public class User {

	/**
	 * 用户ID
	 */
	private int uId;
	/**
	 * 用户昵称
	 */
	private String uNickName;
	/**
	 * 用户名称
	 */
	private String uName;
	/**
	 * 用户手机
	 */
	private String uPhone;
	/**
	 * 用户邮箱
	 */
	private String uEmail;
	/**
	 * 用户密码
	 */
	private String uPassword;
	/**
	 * 用户真实姓名
	 */
	private String uTrueName;
	/**
	 * 用户创建时间
	 */
	private Timestamp uCreateTime;
	/**
	 * 获取用户ID
	 * @return
	 */
	public int getuId() {
		return uId;
	}
	
	/**
	 * 设置用户ID
	 * @param uId
	 */
	public void setuId(int uId) {
		this.uId = uId;
	}
	
	/**
	 * 获取用户昵称
	 * @return
	 */
	public String getuNickName() {
		return uNickName;
	}
	
	/**
	 * 设置用户昵称
	 * @param uNickName
	 */
	public void setuNickName(String uNickName) {
		this.uNickName = uNickName;
	}
	
	/**
	 * 获取用户姓名
	 * @return
	 */
	public String getuName() {
		return uName;
	}
	
	/**
	 * 设置用户姓名
	 * @param uName
	 */
	public void setuName(String uName) {
		this.uName = uName;
	}
	
	/***
	 * 获取用户电话
	 * @return
	 */
	public String getuPhone() {
		return uPhone;
	}
	
	/**
	 * 设置用户电话
	 * @param uPhone
	 */
	public void setuPhone(String uPhone) {
		this.uPhone = uPhone;
	}
	
	/**
	 * 获取用户Email
	 * @return
	 */
	public String getuEmail() {
		return uEmail;
	} 
	
	/**
	 * 设置用户Email
	 * @param uEmail
	 */
	public void setuEmail(String uEmail) {
		this.uEmail = uEmail;
	}

	/**
	 * 获取用户密码
	 * @return
	 */
	public String getuPassword() {
		return uPassword;
	}

	/**
	 * 设置用户密码
	 * @param uPassword
	 */
	public void setuPassword(String uPassword) {
		this.uPassword = uPassword;
	}

	/**
	 * 获取真实用户名
	 * @return 
	 * @author HuYang
	 * @date 2016年11月4日 下午5:13:11
	 */
	public String getuTrueName() {
		return uTrueName;
	}

	/**
	 * 设置真实用户名
	 * @param uTrueName 
	 * @author HuYang
	 * @date 2016年11月4日 下午5:13:34
	 */
	public void setuTrueName(String uTrueName) {
		this.uTrueName = uTrueName;
	}

	/**
	 * 获取用户创建时间
	 * @return 
	 * @author HuYang
	 * @date 2016年11月7日 下午12:20:34
	 */
	public Timestamp getuCreateTime() {
		return uCreateTime;
	}

	/**
	 * 设置用户创建时间
	 * @param uCreateTime 
	 * @author HuYang
	 * @date 2016年11月7日 下午12:20:48
	 */
	public void setuCreateTime(Timestamp uCreateTime) {
		this.uCreateTime = uCreateTime;
	}
	
	
}
